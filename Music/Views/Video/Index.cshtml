@{
    ViewData["Title"] = "";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>播放器</title>
    <link href="//vjs.zencdn.net/7.10.2/video-js.min.css" rel="stylesheet" />


    @*<link href="https://unpkg.com/video.js@7/dist/video-js.min.css"
        rel="stylesheet" />*@

    <!-- City -->
    <link href="https://unpkg.com/@@videojs/themes@1/dist/city/index.css"
          rel="stylesheet" />


    <style>
        #my-player {
            margin: auto;
        }
    </style>
</head>
<body>
    <video id="my-player"
           class="video-js vjs-theme-city"
           controls
           preload="auto"
           poster=""
           data-setup='{}'>
        @*<source  type="application/x-mpegURL">*@
        <p class="vjs-no-js">
            To view this video please enable JavaScript, and consider upgrading to a
            web browser that
            <a href="https://videojs.com/html5-video-support/" target="_blank">
                supports HTML5 video
            </a>
        </p>
    </video>

    <script src="https://cdn.bootcdn.net/ajax/libs/video.js/7.10.1/alt/video.core.min.js"></script>
    <script src="https://cdn.bootcdn.net/ajax/libs/videojs-contrib-hls/5.9.0/videojs-contrib-hls.min.js"></script>
    <script type="text/javascript">
        var myPlayer = videojs('my-player');
        myPlayer.src({ type: 'application/x-mpegURL', src: '/video/01/playlist.m3u8' });

        myPlayer.ready(function () {

        });
        myPlayer.play();
    </script>
</body>
</html>